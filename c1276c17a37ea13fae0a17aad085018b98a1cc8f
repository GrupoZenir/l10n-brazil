    INSERT INTO public.afp_rule_l10n_br_account_partner_fiscal_type(
                afp_id, partner_fiscal_type_id)
         SELECT
            id, partner_fiscal_type_id
         FROM account_fiscal_position_rule
         WHERE partner_fiscal_type_id is not null;
    
